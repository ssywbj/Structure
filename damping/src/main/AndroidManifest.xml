<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suheng.damping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Damping">
        <activity
            android:name="com.suheng.damping.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Damping.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.suheng.damping.SwipeRefreshActivity" />
        <activity android:name="com.suheng.damping.BounceEffectActivity" />
        <activity android:name="com.suheng.damping.DampingActivity" />
        <activity android:name="com.suheng.damping.RebounceEffectsActivity" />
    </application>

</manifest>