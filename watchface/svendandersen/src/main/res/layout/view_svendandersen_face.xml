<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.wiz.watchface.svendandersen.view.PanelView
        android:id="@+id/sa_panel_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.wiz.watchface.svendandersen.view.PanelView
        android:id="@+id/city_panel_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.wiz.watchface.svendandersen.view.ClockPanel
        android:id="@+id/clock_panel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:hourPointer="@drawable/hour_pointer"
        app:minutePointer="@drawable/minute_pointer" />
</merge>