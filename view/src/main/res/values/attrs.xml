<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HourMinuteView">
        <attr name="divider_sign" format="reference" />
        <attr name="time_shade_bg" format="reference" />
    </declare-styleable>

    <!-- DateTimePicker -->
    <attr name="wheelWidth" format="dimension|reference" />

    <declare-styleable name="ListItemLayout">
        <attr name="lil_title" format="reference|string" />
        <attr name="lil_subtitle" format="reference|string" />
        <attr name="lil_sub_progressbar" format="boolean" />
        <attr name="lil_sub_seekbar" format="boolean" />

        <attr name="lil_describe_title" format="reference|string" />
        <attr name="lil_describe_subtitle" format="reference|string" />

        <attr name="lil_left_image" format="reference" />
        <attr name="lil_left_image_dimen" format="enum">
            <enum name="normal" value="0" />
            <enum name="h1" value="1" />
        </attr>

        <attr name="lil_right_layout_type" format="enum">
            <enum name="nothing" value="0" />
            <enum name="custom" value="1" />
            <enum name="image" value="2" />
            <enum name="radio" value="3" />
            <enum name="toggle_switch" value="4" />
        </attr>
        <attr name="lil_right_image" format="reference" />
    </declare-styleable>
</resources>
