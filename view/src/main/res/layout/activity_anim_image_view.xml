<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.suheng.structure.view.AnimImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="4dp" />

    <com.suheng.structure.view.AnimImageView2
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="4dp" />

    <com.suheng.structure.view.AnimImageView3
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="4dp"
        android:src="@drawable/map_my_locating01" />

    <com.suheng.structure.view.AnimImageView3
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="4dp"
        android:src="@drawable/vector_delete" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_selected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Selected"
            android:textAllCaps="false" />

        <com.suheng.structure.view.AnimImageView4
            android:id="@+id/aiv4_selected"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_margin="4dp" />

        <com.suheng.structure.view.AnimImageView4
            android:id="@+id/aiv4_unselected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp" />

        <Button
            android:id="@+id/btn_unselected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Unselected"
            android:textAllCaps="false" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/aiv5_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.suheng.structure.view.AnimImageView5
            android:id="@+id/aiv5_one"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_alarm" />

        <com.suheng.structure.view.AnimImageView5
            android:id="@+id/aiv5_two"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_folder" />

        <com.suheng.structure.view.AnimImageView5
            android:id="@+id/aiv5_three"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_calendar" />

        <com.suheng.structure.view.AnimImageView5
            android:id="@+id/aiv5_four"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_alarm" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/aiv6_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6dp"
        android:gravity="center"
        android:orientation="horizontal">

        <com.suheng.structure.view.AnimImageView6
            android:id="@+id/aiv6_one"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />

        <!--<com.suheng.structure.view.AnimImageView6
            android:id="@+id/aiv6_two"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_folder" />

        <com.suheng.structure.view.AnimImageView6
            android:id="@+id/aiv6_three"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_calendar" />

        <com.suheng.structure.view.AnimImageView6
            android:id="@+id/aiv6_four"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:src="@drawable/icon_alarm" />-->
    </LinearLayout>
</LinearLayout>
