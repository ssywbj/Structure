<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suheng.structure.module3">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.Module3App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/UIAppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MVCLoginActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MVPLoginActivity"
            android:label="MVP Activity"
            android:launchMode="singleTask" />

        <activity
            android:name=".FragmentActivity"
            android:label="Fragment Apply"
            android:launchMode="singleTask" />

        <activity
            android:name=".application.Module3MainActivity"
            android:launchMode="singleTask" />
    </application>

</manifest>